{
  "name": "transaction-monitoring-service",
  "version": "1.0.0",
  "description": "Service for monitoring blockchain transactions in ETH, TRON, and other networks",
  "main": "dist/main.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "node dist/main.js",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "dev": "ts-node -r tsconfig-paths/register src/main.ts",
    "watch": "tsc --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "check-eth-tx": "ts-node -r tsconfig-paths/register src/check-eth-transaction.ts",
    "test-sheets": "ts-node -r tsconfig-paths/register src/test-google-sheets.ts"
  },
  "keywords": [
    "tron",
    "ethereum",
    "eth",
    "blockchain",
    "transactions",
    "monitoring"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^10.3.6",
    "@nestjs/core": "^10.3.6",
    "axios": "^1.6.7",
    "dotenv": "^16.3.1",
    "ethers": "^6.0.0",
    "googleapis": "^148.0.0",
    "moment": "^2.29.4",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "tronweb": "^5.3.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  }
}